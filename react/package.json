{
  "name": "canvas-embed",
  "version": "1.0.15",
  "description": "Embed your Canvas charts in your application",
  "scripts": {
    "dev": "webpack serve --config=webpack.dev.cjs",
    "build": "yarn run clean-dist && webpack --config=webpack.prod.cjs",
    "build-watch": "tsup src/index.tsx --watch --format cjs,esm --dts",
    "tsc": "tsc",
    "tsc2": "tsc -m es6 --outDir lib-esm",
    "clean-dist": "rimraf dist/*",
    "prettier": "prettier . --write"
  },
  "author": "Will Pride <will@canvasapp.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canvas/embed.git"
  },
  "bugs": {
    "url": "https://github.com/canvas/embed/issues"
  },
  "homepage": "https://github.com/canvas/embed",
  "license": "MIT",
  "files": [
    "dist/*",
    "package.json"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "env": {
      "browser": true,
      "es6": true
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "source": "src/Chart.tsx",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-typescript": "^7.23.3",
    "@types/chroma-js": "^2.4.2",
    "@types/lodash": "^4.14.202",
    "@types/luxon": "^3.3.3",
    "@types/react": "^18.2.43",
    "@types/react-window": "^1.8.8",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "less-loader": "^11.1.3",
    "postcss-loader": "^7.3.3",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.0.3",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "sideEffects": [
    "dist/*",
    "styles/*"
  ],
  "peerDependencies": {
    "chroma-js": "^2.4.2",
    "highcharts": "^11.2.0",
    "highcharts-react-official": "^3.2.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "percentile": "^1.6.0",
    "react": "^18.2.0",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.10"
  },
  "dependencies": {
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.32"
  }
}
