{
  "name": "canvas-embed",
  "version": "1.0.53",
  "description": "Embed your Canvas charts in your application",
  "main": "dist/bundle.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "NODE_ENV=development webpack --config webpack.config.js --watch",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "build:css": "npx tailwindcss -i ./src/index.css -o ./dist/output.css",
    "build:css:watch": "npx tailwindcss -i ./src/index.css -o ./dist/output.css --watch",
    "copy-rust-types": "rm -rf ./src/rust_types/* && cp -r ../../sheets/frontend/src/__rust_generated__/* ./src/rust_types",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "prettier": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "author": "Will Pride <will@canvasapp.com>",
  "contributors": [
    "Jeremy Bernier <jeremy@canvasapp.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canvas/embed.git"
  },
  "bugs": {
    "url": "https://github.com/canvas/embed/issues"
  },
  "homepage": "https://github.com/canvas/embed",
  "license": "MIT",
  "keywords": [
    "canvas",
    "embed",
    "charts",
    "react",
    "typescript"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/chroma-js": "^2.4.3",
    "@types/lodash": "^4.14.202",
    "@types/luxon": "^3.3.7",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "less-loader": "^11.1.3",
    "postcss-loader": "^7.3.3",
    "postcss-nesting": "^12.0.1",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.1",
    "tailwindcss-themer": "^3.0.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "antd": "^5.12.2",
    "chroma-js": "^2.4.2",
    "highcharts": "^11.2.0",
    "highcharts-react-official": "^3.2.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "percentile": "^1.6.0",
    "postcss": "^8.4.32",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.10",
    "slugify": "^1.6.6",
    "watch": "^0.13.0",
    "zustand": "^4.4.7"
  }
}
